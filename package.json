{
  "name": "my-pets-api",
  "version": "0.0.12",
  "description": "RESTful api based on NodeJs to give middle/back-end support a React web application",
  "author": "Nicolás Omar González Passerino",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/NicolasOmar/my-pets-api#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasOmar/my-pets-api.git"
  },
  "bugs": {
    "url": "https://github.com/NicolasOmar/my-pets-api/issues"
  },
  "scripts": {
    "start": "env-cmd -f ./env/local.env nodemon src/index.js --ext js,gql --exec babel-node",
    "start:ci": "babel ./src/index.js -d ./build ./src && node ./build/index.js",
    "lint": "eslint --fix src",
    "prettier": "prettier --write src",
    "update-version": "node ./scripts/update-version.js"
  },
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.13.15",
    "apollo-server-errors": "^2.5.0",
    "apollo-server-express": "^2.21.0",
    "babel-plugin-import-graphql": "^2.8.1",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "hbs": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.8",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.4",
    "env-cmd": "^10.1.0",
    "eslint": "^7.16.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "engines": {
    "node": "14.17.0",
    "npm": "7.15.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "src/**/!(*test).js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run update-version"
    }
  }
}
